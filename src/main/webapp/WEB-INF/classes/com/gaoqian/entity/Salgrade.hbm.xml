<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gaoqian.entity.Salgrade">
        <id name="grade" type="java.lang.Integer">
            <column name="grade"></column>
        </id>
        <property name="losal" type="java.lang.String">
            <column name="losal"></column>
        </property>
        <property name="hisal" type="java.lang.String">
            <column name="hisal"></column>
        </property>

    </class>


</hibernate-mapping>